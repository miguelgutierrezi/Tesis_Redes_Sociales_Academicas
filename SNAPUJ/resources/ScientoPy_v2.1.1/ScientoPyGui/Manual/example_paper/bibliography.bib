  ISBN                     = {9781467398039},
  Journal                  = {Proceedings - 2016 International Conference on Networking and Network Applications, NaNA 2016},
  Keywords                 = {Bluetooth; Health care; HTTP; Internet; Internet protocols; Low power electronics; Network architecture, 6LoWPAN; Bluetooth low energies (BLE); CoAP; Constrained Application Protocol (CoAP); Efficient communications; Internet of thing (IOT); ISO/IEEE 11073; Testbed experimentations, Internet of things},
  Language                 = {English},
