@Article{Zhang2018,
  Title                    = {Efficient large-universe multi-authority ciphertext-policy attribute-based encryption with white-box traceability},
  Author                   = {Zhang, K. and Li, H. and Ma, J. and Liu, X.},
  Journal                  = {Science China Information Sciences},
  Year                     = {2018},
  Number                   = {3},
  Volume                   = {61},

  Abbrev_source_title      = {Sci. China Inf. Sci.},
  Abstract                 = {Traceable multi-authority ciphertext-policy attribute-based encryption (CP-ABE) is a practical encryption method that can achieve user traceability and fine-grained access control simultaneously. However, existing traceable multi-authority CP-ABE schemes have two main limitations that prevent them from practical applications. First, these schemes only support small universe: the attributes must be fixed at system setup and the attribute space is restricted to polynomial size. Second, the schemes are either less expressive (the access policy is limited to “AND gates with wildcard”) or inefficient (the system is constructed in composite order bilinear groups). To address these limitations, we present a traceable large universe multi-authority CP-ABE scheme, and further prove that it is statically secure in the random oracle model. Compared with existing traceable multi-authority CP-ABE schemes, the proposed scheme has four advantages. First, the attributes are not fixed at setup and the attribute universe is not bounded to polynomial size. Second, the ciphertext polices can be expressed as any monotone access structures. Third, the proposed scheme is constructed in prime order groups, which makes this scheme more efficient than those in composite order bilinear groups. Finally, the proposed scheme requires neither a central authority nor an identity table for tracing. © 2017, Science China Press and Springer-Verlag GmbH Germany.},
  Affiliation              = {School of Telecommunications Engineering, Xidian University, Xi’an, China; School of Cyber Engineering, Xidian University, Xi’an, China; School of Computer Science and Technology, Xidian University, Xi’an, China; School of Information Systems, Singapore Management University, Singapore, Singapore},
  Art_number               = {032102},
  Author_keywords          = {attribute-based encryption; ciphertext-policy; large universe; multi-authority; traceability},
  Correspondence_address1  = {Ma, J.; School of Computer Science and Technology, Xidian UniversityChina; email: jfma@mail.xidian.edu.cn},
  Document_type            = {Article},
  Doi                      = {10.1007/s11432-016-9019-8},
  ISSN                     = {1674733X},
  Keywords                 = {Access control; Security of data, Attribute-based encryptions; Ciphertext policies; large universe; Multi authorities; traceability, Cryptography},
  Language                 = {English},
  Publisher                = {Science in China Press},
  Source                   = {Scopus},
  Url                      = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85021648834&doi=10.1007%2fs11432-016-9019-8&partnerID=40&md5=f642f21942665ff58519a8ed3c82c4a0}
}

