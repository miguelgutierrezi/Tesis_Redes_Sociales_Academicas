@Article{Burmester2014317,
  Title                    = {Pre vs post state update: Trading privacy for availability in RFID},
  Author                   = {Burmester, M. and Munilla, J.},
  Journal                  = {IEEE Wireless Communications Letters},
  Year                     = {2014},
  Number                   = {3},
  Pages                    = {317-320},
  Volume                   = {3},

  Abbrev_source_title      = {IEEE Wireless Commun. Lett.},
  Abstract                 = {Designing lightweight RFID protocols that support strong privacy is a major challenge. For anonymity tags use pseudonyms that are refreshed with every interrogation (whether completed or not). For forward secrecy, the state of tags must be updated and it must be hard to reverse updates. Since the interrogating reader can be adversarial, the adversary may control state updates. It follows that it may not be possible for tags to maintain synchrony with authorized readers. In this letter we analyze a recently proposed RFID protocol and show that there is a fundamental trade-off between privacy and availability. We prove that for lightweight RFID applications strong privacy cannot be achieved in the presence of a Byzantine adversary. © 2012 IEEE.},
  Affiliation              = {Department of Computer Science, Florida State University, Tallahassee, FL, 30302, United States; Communication Engineering Department, Univ. de Málaga, Spain},
  Art_number               = {6783814},
  Author_keywords          = {de-synchronization; DoS; forward secrecy; privacy; protocol failure; RFID; unlinkability},
