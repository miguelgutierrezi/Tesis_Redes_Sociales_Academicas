  ISBN                     = {9781467365093},
  Journal                  = {2014 20th Annual International Conference on Advanced Computing and Communications, ADCOM 2014 - Proceedings},
  Keywords                 = {Internet; Internet protocols; Security of data, Application scenario; CoAP; DTLS; Lightweight securities; M2M; Security domains; Security features; Transport layer security protocols, Internet of things},
  Language                 = {English},
  Source                   = {Scopus},
