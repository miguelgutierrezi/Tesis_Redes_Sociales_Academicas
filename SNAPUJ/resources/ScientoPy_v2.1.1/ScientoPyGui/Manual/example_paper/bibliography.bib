  Journal                  = {Proceedings - 2013 IEEE International Conference on Green Computing and Communications and IEEE Internet of Things and IEEE Cyber, Physical and Social Computing, GreenCom-iThings-CPSCom 2013},
  Keywords                 = {Android; Context-awareness; Internet of Things (IOT); OSGi; Web of things, Internet protocols; Mobile devices; Search engines; World Wide Web, Gateways (computer networks)},
  Language                 = {English},
  Source                   = {Scopus},
