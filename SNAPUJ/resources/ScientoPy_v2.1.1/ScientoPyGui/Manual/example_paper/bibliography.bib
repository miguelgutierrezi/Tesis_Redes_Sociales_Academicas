  Document_type            = {Article},
  Doi                      = {10.1016/j.adhoc.2013.02.007},
  ISSN                     = {15708705},
  Keywords                 = {Algorithms; Congestion control (communication); Design; Internet; Network architecture; Sensors; Web services; Websites, 6LoWPAN; Back pressures; CoAP; Internet of Things (IOT); Protocol design, Internet protocols},
  Language                 = {English},
  Publisher                = {Elsevier},
