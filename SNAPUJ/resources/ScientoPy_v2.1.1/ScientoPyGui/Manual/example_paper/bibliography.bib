  ISSN                     = {21622337},
  Keywords                 = {Data privacy; DOS; Electrical engineering; Electronics engineering; Radio frequency identification (RFID), Control state; Desynchronization; Forward secrecy; RFID applications; RFID protocols; Unlinkability, Internet of things},
  Language                 = {English},
  Publisher                = {Institute of Electrical and Electronics Engineers Inc.},
  Source                   = {Scopus},
  Url                      = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84903275980&doi=10.1109%2fWCL.2014.032814.140043&partnerID=40&md5=d048c386382f40edf2b14cb6a2d287bb}
}

