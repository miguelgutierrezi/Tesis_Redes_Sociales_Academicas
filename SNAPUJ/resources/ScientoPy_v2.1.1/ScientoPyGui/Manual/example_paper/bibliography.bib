  ISBN                     = {9781479917747},
  Journal                  = {Proceedings - IEEE 29th International Conference on Advanced Information Networking and Applications Workshops, WAINA 2015},
  Keywords                 = {Ad hoc networks; Heterogeneous networks; Internet; Internet of things; Network architecture; Sensor networks; Wireless ad hoc networks, Distributed architecture; Exponential growth; Internet of Things (IOT); Multiple domains; SDN; Security; Security networks; Software defined networking (SDN), Network security},
  Language                 = {English},
  Source                   = {Scopus},
  Url                      = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84947710564&doi=10.1109%2fWAINA.2015.110&partnerID=40&md5=d8ad54183b80fcfe776017778078cf18}
