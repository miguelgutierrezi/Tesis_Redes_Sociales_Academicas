  Author_keywords          = {Atmospheric environment monitoring; Dust sensor; Fire-watching; Image processing; Microcontroller},
  Correspondence_address1  = {Park, J.-H.; Dept. of Control and Instrumentation Eng, Korea National Univ. of Transportation, #50 Daehak-ro, South Korea; email: jhpark@ut.ac.kr},
  Document_type            = {Article},
