@InProceedings{7808003,
  Title                    = {Design and development of an IOT based wearable device for the safety and security of women and girl children},
  Author                   = {A. Jatti and M. Kannan and R. M. Alisha and P. Vijayalakshmi and S. Sinha},
  Booktitle                = {2016 IEEE International Conference on Recent Trends in Electronics, Information Communication Technology (RTEICT)},
  Year                     = {2016},
  Month                    = {May},
  Pages                    = {1108-1112},

  Abstract                 = {The aim of this work is to develop a wearable device for the safety and protection of women and girls. This objective is achieved by the analysis of physiological signals in conjunction with body position. The physiological signals that are analyzed are galvanic skin resistance and body temperature. Body position is determined by acquiring raw accelerometer data from a triple axis accelerometer. Acquisition of raw data is then followed by activity recognition which is a process of employing a specialized machine learning algorithm. Real-time monitoring of data is achieved by wirelessly sending sensor data to an open source Cloud Platform. Analysis of the data is done on MATLAB simultaneously. This device is programmed to continuously monitor the subject's parameters and take action when any dangerous situation presents itself. It does so by detecting the change in the monitored signals, following which appropriate action is taken by means of sending notifications/alerts to designated individuals.},
  Doi                      = {10.1109/RTEICT.2016.7808003},
  Keywords                 = {Internet of Things;accelerometers;cloud computing;learning (artificial intelligence);ubiquitous computing;wearable computers;IOT;accelerometer data;body position;body temperature;galvanic skin resistance;girl children;machine learning algorithm;monitored signals;open source cloud platform;physiological signals;real-time monitoring;subject parameters;triple axis accelerometer;wearable device;women children;Accelerometers;Biomedical monitoring;Conferences;Immune system;Skin;Stress;Temperature sensors;Activity recognition;Body Temperature;Galvanic Skin Resistance;Internet of Things;Machine Learning;WEKA}
}

