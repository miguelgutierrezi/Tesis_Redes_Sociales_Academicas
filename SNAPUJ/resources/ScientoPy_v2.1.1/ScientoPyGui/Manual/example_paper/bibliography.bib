@Article{Wang2016173,
  Title                    = {Storage-Less and Converter-Less Photovoltaic Energy Harvesting with Maximum Power Point Tracking for Internet of Things},
  Author                   = {Wang, Y. and Liu, Y. and Wang, C. and Li, Z. and Sheng, X. and Lee, H.G. and Chang, N. and Yang, H.},
  Journal                  = {IEEE Transactions on Computer-Aided Design of Integrated Circuits and Systems},
  Year                     = {2016},
  Number                   = {2},
  Pages                    = {173-186},
  Volume                   = {35},

  Abbrev_source_title      = {IEEE Trans Comput Aided Des Integr Circuits Syst},
  Abstract                 = {Energy harvesting from natural environment gives range of benefits for the Internet of things. Scavenging energy from photovoltaic (PV) cells is one of the most practical solutions in terms of power density among existing energy harvesting sources. PV power systems mandate the maximum power point tracking (MPPT) to scavenge the maximum possible solar energy. In general, a switching-mode power converter, an MPPT charger, controls the charging current to the energy storage element (a battery or equivalent), and the energy storage element provides power to the load device. The mismatch between the maximum power point (MPP) current and the load current is managed by the energy storage element. However, such architecture causes significant energy loss (typically over 20%) and a significant weight/volume and a high cost due to the cascaded power converters and the energy storage element. This paper pioneers a converter-less PV power system with the MPPT that directly supplies power to the load without the power converters or the energy storage element. The proposed system uses a nonvolatile microprocessor to enable an extremely fine-grain dynamic power management in a few hundred microseconds. This makes it possible to match the load current with the MPP current. We present detailed modeling, simulation, and optimization of the proposed energy harvesting system including the radio frequency transceiver. Experiments show that the proposed setup achieves an 87.1% of overall system efficiency during a day, 30.6% higher than the conventional MPPT methods in actual measurements, and thus a significantly higher duty cycle under a weak solar irradiance. © 1982-2012 IEEE.},
  Affiliation              = {Department of Electronic Engineering, Tsinghua University, Beijing, China; Texas Instrument, Dallas, TX, United States; Daegu University, Gyeongsan, South Korea; Department of Electrical Engineering, Korea Advanced Institute of Science and Technology, Daejeon, South Korea},
  Art_number               = {7128333},
  Author_keywords          = {Energy efficiency; Energy harvesting; Maximum-power-point tracking (MPPT); Photovoltaic systems; Storage-less and converter-less},
  Coden                    = {ITCSD},
  Correspondence_address1  = {Liu, Y.; Department of Electronic Engineering, Tsinghua UniversityChina; email: ypliu@tsinghua.edu.cn},
  Document_type            = {Article},
  Doi                      = {10.1109/TCAD.2015.2446937},
  ISSN                     = {02780070},
  Keywords                 = {Charging (batteries); Energy dissipation; Energy efficiency; Energy storage; Internet; Internet of things; Maximum power point trackers; Photovoltaic cells; Power converters; Radio transceivers; Solar energy; Solar power generation, Cascaded power converters; Energy harvesting systems; Maximum power point; Maximum Power Point Tracking; Natural environments; Photovoltaic energy harvesting; Photovoltaic systems; Radio frequency transceiver, Energy harvesting},
  Language                 = {English},
  Publisher                = {Institute of Electrical and Electronics Engineers Inc.},
  Source                   = {Scopus},
  Url                      = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84962343680&doi=10.1109%2fTCAD.2015.2446937&partnerID=40&md5=a8884b396fe6763404ccf87e2607ba62}
