  ISSN                     = {20488882},
  Journal                  = {Proceedings of the International Conference on e-Learning, ICEL},
  Keywords                 = {Augmented reality; E-learning; Education; Education computing; Internet; Mobile devices; Smartphones; Students; Teaching; Telecommunication equipment, Amount of information; Bluetooth transmitters; Communication method; Distributed knowledge; Dynamic presentation; Electronic information; Electronic technologies; Mobile Technology, Engineering education},
  Language                 = {English},
