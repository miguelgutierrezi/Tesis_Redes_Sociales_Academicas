  Correspondence_address1  = {Buyya, R.; Department of Computing and Information Systems, University of Melbourne, VIC 3010, Australia; email: rbuyya@unimelb.edu.au},
  Document_type            = {Article},
  Doi                      = {10.1016/j.future.2013.01.010},
  ISSN                     = {0167739X},
  Keywords                 = {Common operating pictures; Environmental indicators; Internet of thing (IOT); Internet of Things (IOT); Revolutionary technology; Smart environment; Technological researches; Ubiquitous sensing, Actuators; Cloud computing; Digital storage; Ubiquitous computing; Wireless sensor networks; Wireless telecommunication systems, Internet},
  Language                 = {English},
